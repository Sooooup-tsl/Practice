.stamp {
    color: #fac8c8;
    border: 1px dashed #fac8c8;
    font-size: 10px;
    width: 50px;
    height: 50px;
    border-radius: 50px;
    line-height: 48px;
    text-align: center;
    margin: auto;
    transform: translateY(7px);
}

.stamp-border {
    content: '';
    width: 70px;
    height: 70px;
    border: 3px solid #fac8c8;
    border-radius: 70px;
    transform: rotate(-30deg);
    position: absolute;
    top: -30px;
    right: 12%;
}

.arrow:after {

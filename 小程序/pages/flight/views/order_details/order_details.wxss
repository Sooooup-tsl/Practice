}

.popup .popup-close {
    position: fixed;
		bottom: 0;
		left: 0;
		right: 0;
    color: #fff;
    text-align: center;
		height: 70px;
		background: white;
		box-shadow:-2px -3px 8px white;
}
.popup .popup-close .close-button {
    box-sizing: content-box;
    background-color: #daf5da;
    height: 28px;
    width: 28px;
    left: 50%;
		transform: translate(-50%, -50%);
		top: 50%;
		position: absolute;
    border-radius: 100%;
    border: 1px solid #b5eab4;
}
.popup .popup-close .close-button:before {
    content: "";
    border-left: 1px solid #09bb07;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    position: absolute;
    top: 6px;
    left: 50%;
    height: 16px;
}
.popup .popup-close .close-button:after {
    content: "";
    border-left: 1px solid #09bb07;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    position: absolute;
    top: 6px;
    left: 50%;
    height: 16px;
}


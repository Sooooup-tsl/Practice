.popup .content .trace-list:first-child:before {
	background: #09bb07;
	width: 9px;
	height: 9px;
	border: 2px solid #daf5da;
	border-radius: 100%;
}

.popup .content .trace-list:before {
	content: '';
	position: absolute;
	display: block;
	width: 11px;
	height: 11px;
	background: #dcdcdc;
	border-radius: 100%;

.location::before{
    content: '当前';
    position: relative;
    bottom: 2px;
    display: inline-block;
    padding: 0 5px;
    margin-right: 5px;
    height: 16px;
    font-size: 12px;
    line-height: 16px;
    color: #fff;
    background: #23beae;
    border: none;
    border-radius: 5px;
}
#startAddress_local view,
#startAddress_nearby view,
.startAddress_history_box view {
    font-size: 12px;
    color: #999;
}
.startAddress_address_box {
    position: relative;
    /*margin-top: 10px;*/
    background-color: #FFF;
    /* border: 1px solid #DDD; */
}
.startAddress_address_box view {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    /*height: 60px;*/
    color: #333;
}
.startAddress_place {
    font-size: 16px;
    /*height: 18px;*/
    line-height: 18px;
}
.startAddress_address {
    font-size: 12px;
    /*height: 14px;*/
    line-height: 14px;
    margin-top: 3px;
    color: #666;
}
.pageABC .startAddress_address_box .startAddress_address_li{
    position: relative;
    padding: 12.5px 10px 12.5px 35px;
}
.pageABC .startAddress_address_box .startAddress_address_li::before{
    content: "";
    position: absolute;
    left: 10px;
}
.startAddress_local_address .startAddress_address_li::before {
    top: 50%;
    width: 14px;
    height: 18px;
    margin: -9px 0 0 0;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/locationIcon.png?v=20170411") no-repeat;
    background-size: 100%;
}
.startAddress_nearby_address view::before {
    top: 50%;
    width: 14px;
    height: 18px;
    margin: -9px 0 0 0;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/nearby.png?v=2016111201") no-repeat;
    background-size: 100%;
}
.startAddress_history view::before {
    top: 50%;
    width: 16px;
    height: 16px;
    margin: -8px 0 0 0;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/history_address.png?v=2016111201") no-repeat;
    background-size: 100%;
}
.startAddress_train view::before {
    top: 50%;
    width: 14px;
    height: 23px;
    margin: -12px 0 0 0;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/qita.png?v=2016111201") no-repeat;
    background-size: 100%;
}
.startAddress_hotel view::before {
    top: 50%;
    width: 16px;
    height: 18px;
    margin: -8px 0 0 0;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/hotel.png?v=2017030601") no-repeat;
    background-size: 100%;
}
.pageABC .startAddress_address_box .startAddress_address_li::after,
.pageD .startAddress_address_box .startAddress_address_li::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 1px;
    background-color: #F1F1F1;
}
.pageD .startAddress_address_box .startAddress_address_li {
    padding: 12.5px 15px;
    position: relative;
} 
.startAddress_city{
    position: relative;
    background-color: #FFF;
    display: inline-block;
    height: 45px;
    line-height: 45px;
    width: 80px;
    padding-right: 10px;
    font-size: 14px;
    text-align: center;
    color: #333;
    box-sizing: border-box;
}

.startAddress_city::before{
    position: absolute;
    content: '';
    height: 25px;
    background: #dcdcdc;
    width: 1px;
    top: 10px;
    left: 80px;
}
.startAddress_city::after {
    display: block;
    content: "";
    width: 6px;
    height: 6px;
    border-top: 1px solid #999;
    border-right: 1px solid #999;
    -webkit-transform: rotate(135deg);
    -moz-transform: rotate(135deg);
    -ms-transform: rotate(135deg);
    -o-transform: rotate(135deg);
    transfrom: rotate(135deg);
    position: absolute;
    top: 50%;
    right: 12px;
    margin-top: -5.5px;
}
.startAddress_search_box {
    position: relative;
    font-size: 0;
    border-top: 6px solid #f2f4f7;
    border-bottom: 1px solid #F1F1F1;
    margin-bottom: 5px;
    background: #fff;
}
.startAddress_search {
    width: 65%;
    width: calc(100% - 80px);
    font-size: 16px;
    height: 25px;
    line-height: 25px;
    margin-top: 10px;
    border: 0 none;
    padding-left: 34px;
    background-color: #FFF;
    vertical-align: top;
}
.startAddress_icon {
    position: absolute;
    left: 90px;
    top: 15px;
    width: 14px;
    height: 14px;
    display: none;
}
.startAddress_delete {
    width: 16px;
    height: 16px;
    position: absolute;
    top: 0;
    right: 0;
    display: none;
    padding: 15px 10px;
}
.startAddress_placeholder {
    width: 120px;
    height: 45px;
    color: #999;
    line-height: 45px;
    position: absolute;
    top: 0;
    /*left: 50%;*/
    /*margin-left: -107px;*/
    margin-left: 80px;
    padding-left: 40px;
    font-size: 16px;
}
.startAddress_placeholder::after {
    content: "";
    position: absolute;
    left: 10px;
    top: 15px;
    width: 14px;
    height: 14px;
    background: url("https://img1.40017.cn/cn/TC_Car/h5/img/index/search_icon.png?v=2016111201") no-repeat;
    background-size: 100%;
}
